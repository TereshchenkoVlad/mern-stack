{
  "name": "mern-stack",
  "version": "1.0.0",
  "description": "Mern stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TereshchenkoVlad/mern-stack.git"
  },
  "keywords": [
    "mern",
    "react",
    "js",
    "mongo"
  ],
  "author": "Vladyslav Tereshchenko <terhoha@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TereshchenkoVlad/mern-stack/issues"
  },
  "homepage": "https://github.com/TereshchenkoVlad/mern-stack#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.2.5",
    "express": "^4.18.2",
    "express-validator": "^6.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.10",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "nodemon": "^2.0.2"
  }
}
